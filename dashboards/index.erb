    <li data-row="2" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="cats" data-view="RandomCats"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-view="Countdown" data-title="Birthday" data-end="29-September-<%= Time.new.year %> 00:15:00"></div>

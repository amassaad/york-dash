<% content_for :title do %>York Dash<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="2">
      <img src="https://media.giphy.com/media/FqtWrearu5vb2/giphy.gif" style='width:100%;' border="0" alt="york">
